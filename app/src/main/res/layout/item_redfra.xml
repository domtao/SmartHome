<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/item_height110"
    android:layout_margin="@dimen/item_height15"
    android:background="@drawable/bg_gv" >

        <TextView
            android:id="@+id/text"
            android:layout_marginTop="@dimen/item_height15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:textColor="@color/commo_text_color"
            android:textSize="@dimen/font_size_16" />
        <ImageView
            android:id="@+id/img"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_margin="@dimen/item_height10"
            android:src="@mipmap/ch_edite"/>


</RelativeLayout>