<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/dialog_admin1"
    android:layout_width="@dimen/item_height320"
    android:layout_height="@dimen/item_height160"
    android:orientation="vertical" >
    <RelativeLayout
        android:layout_width="@dimen/item_height320"
        android:background="@mipmap/zun_title_bg"
        android:layout_height="@dimen/item_height50">
        <TextView
            android:layout_centerInParent="true"
            android:id="@+id/msgTitle"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="@string/tip"
            android:gravity="center"
            android:textColor="@color/white"
            android:textSize="@dimen/font_size_16" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="@dimen/item_height320"
        android:layout_height="@dimen/item_height60"
        android:orientation="horizontal"
        android:paddingLeft="10dp"
        android:background="@mipmap/zun_body_bg"
        android:paddingRight="10dp" >
        <TextView
            android:layout_centerVertical="true"
            android:id="@+id/msgTxt"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="正在获取设备 10"
            android:textColor="@color/black"
            android:textSize="@dimen/font_size_14" />
    </RelativeLayout>

    <View
    android:layout_width="@dimen/item_height320"
    android:layout_height="1dp"
    android:background="@color/add_line"/>
    <LinearLayout
        android:layout_width="@dimen/item_height320"
        android:layout_height="@dimen/item_height50"
        android:background="@mipmap/zun_botton_bg"
        android:orientation="horizontal">
        <Button
            android:id="@+id/cancle_bt"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="@string/cancle"
            android:background="@color/transparent"
            android:textColor="@color/font_close_press" />

    </LinearLayout>


</LinearLayout>