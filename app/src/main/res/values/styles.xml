<resources>

    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.Main" />

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
    <style name="mypopwindow_anim_style">
        <item name="android:windowEnterAnimation">@anim/popshow_anim</item>
        <!-- 指定显示的动画xml -->

        <item name="android:windowExitAnimation">@anim/pophidden_anim</item>
        <!-- 指定消失的动画xml -->
    </style>

    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <style name="Theme.AppStartLoad" parent="android:Theme">
        <item name="android:windowBackground">@mipmap/start_image</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="Theme.AppStart" parent="android:Theme">
        <item name="android:windowBackground">@mipmap/start_image</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <!-- Base application theme is the default theme. -->
    <style name="Theme" parent="android:Theme"></style>

    <style name="CustomDialogTheme" parent="@android:style/Theme.Dialog"></style>

    <style name="popwin_anim_style">
        <item name="android:windowEnterAnimation">@anim/popu_in</item>
        <item name="android:windowExitAnimation">@anim/popu_out</item>
    </style>
    <!-- QQ -->
    <style name="ButtomBar">
        <item name="android:layout_height">60dp</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:padding">5dp</item>
    </style>

    <style name="ButtomBarImgBtn">
        <item name="android:layout_height">60dp</item>
        <item name="android:layout_width">55dp</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:layout_centerInParent">true</item>
    </style>

    <style name="scan_dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowBackground">@color/whites</item>
    </style>

    <!-- 投影机 -->
    <style name="layout_matchparent">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>

    <style name="stb_layout_main_projector" parent="@style/layout_matchparent">
        <item name="android:paddingLeft">10.0dip</item>
        <item name="android:paddingRight">10.0dip</item>
    </style>

    <style name="projector_controlview">
        <item name="android:layout_marginTop">5.0dip</item>
    </style>

    <style name="projector_controlview_button">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="projector_controlview_content_button">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="projector_controlview_content_textview">
        <item name="android:textSize">8.0pt</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="projector_controlview_relative">
        <item name="android:layout_marginTop">5.0dip</item>
        <item name="android:layout_marginBottom">10.0dip</item>
    </style>

    <style name="projector_controlview_disk">
        <item name="android:layout_width">200.0dip</item>
        <item name="android:layout_height">200.0dip</item>
    </style>

    <style name="dvd_controlview_diskrelative">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">5.0dip</item>
        <item name="android:layout_marginBottom">5.0dip</item>
    </style>

    <style name="dvd_controlview_disk_button">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="dvd_controlview_disk_buttonok">
        <item name="android:textSize">22.0sp</item>
        <item name="android:layout_width">@dimen/item_height60</item>
        <item name="android:layout_height">60dp</item>
    </style>

    <style name="dvd_controlview_disk_buttonHorizontal">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="dvd_controlview_disk_buttonVertical">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="dvd_controlview_disk_playbutton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="dvd_controlview_button">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="dvd_controlview_usebutton">
        <item name="android:paddingLeft">10.0dip</item>
        <item name="android:paddingRight">10.0dip</item>
        <item name="android:layout_width">300.0dip</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="dvd_controlview_disk">
        <item name="android:padding">7.0dip</item>
        <item name="android:layout_width">185.0dip</item>
        <item name="android:layout_height">185.0dip</item>
        <item name="android:layout_marginLeft">20.0dip</item>
        <item name="android:layout_marginRight">20.0dip</item>
    </style>

    <style name="stb_bluetoothview_bottom" parent="@style/layout_matchparent">
        <item name="android:layout_gravity">bottom</item>
    </style>

    <style name="stb_bluetoothview_bottom_home" parent="@style/layout_wrapcontent" />

    <style name="layout_wrapcontent">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="stb_bluetoothview_bottom_edit" parent="@style/layout_wrapcontent" />

    <style name="stb_bluetoothview_bottom_change" parent="@style/layout_wrapcontent">
        <item name="android:paddingBottom">5.0dip</item>
    </style>

    <style name="stb_layout_main" parent="@style/layout_matchparent">
        <item name="android:paddingLeft">10.0dip</item>
        <item name="android:paddingRight">10.0dip</item>
    </style>

    <style name="tv_controlview_diskrelative">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">5.0dip</item>
        <item name="android:layout_marginBottom">5.0dip</item>
    </style>

    <style name="mystyle_IrForm">
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="music_controlview">
        <item name="android:padding">7.0dip</item>
        <item name="android:layout_width">160.0dip</item>
        <item name="android:layout_height">160.0dip</item>
        <item name="android:layout_marginLeft">5.0dip</item>
        <item name="android:layout_marginRight">5.0dip</item>
    </style>

    <style name="music_controlview_relative">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">5.0dip</item>
        <item name="android:layout_marginBottom">5.0dip</item>
    </style>

    <!-- 摄像头 -->
    <style name="bg_style">
        <item name="android:background">#ffeae6e3</item>
    </style>
    <!-- 自定义下拉列表 -->
    <style name="text_whitebtn_style">
        <item name="android:textSize">18.0dip</item>
        <item name="android:textColor">#ff444444</item>
    </style>

    <style name="dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.6</item>
    </style>

    <style name="MyDialog">
        <item name="android:windowFrame">@null</item>
        <!-- 最后面是window background -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">false</item>
        <!-- 这个不设置它会沾满屏幕 -->
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <!-- 设置contentview的背景图 -->
    </style>
    <!-- 重写系统弹出Dialog -->
    <style name="menuDialogTheme" parent="android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item> <!-- 除去title -->
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowBackground">@null</item> <!-- 除去背景色 -->
        <item name="android:windowFullscreen">true</item>
    </style>

    <!-- 重写系统弹出Dialog -->
    <style name="myDialogTheme" parent="android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item> <!-- 除去title -->
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowBackground">@null</item> <!-- 除去背景色 -->
    </style>

    <style name="main_tab_bottom">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:paddingTop">5.0dip</item>
        <item name="android:paddingBottom">2.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">2.0dip</item>
        <item name="android:button">@null</item>
        <item name="android:singleLine">true</item>
        <item name="android:drawablePadding">2.0dip</item>
        <item name="android:layout_weight">1.0</item>
    </style>

    <style name="GlobeTextView" parent="@android:style/Widget.TextView">
        <item name="android:textColor">@color/backColor</item>
        <item name="android:textSize">16.0sp</item>
    </style>
    <style name="GlobeTheme" parent="@android:style/Theme">
        <item name="android:windowNoTitle">true</item>
        <item name="android:textViewStyle">@style/GlobeTextView</item>
    </style>
</resources>
