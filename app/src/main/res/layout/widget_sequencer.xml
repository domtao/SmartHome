        <LinearLayout
            android:visibility="gone"
            android:id="@+id/layout_all"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:background="@drawable/bg_gv"
